<% include ./header.ejs %>
<br><br><br>
    <div class="container">
        <div class="col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2">
            <div class="panel panel-success">
                <div class="panel-heading">
                    <div class="panel-title">
                        Fooriend
                    </div>
                </div>
                <div class="panel-body">
                    <form action="/register" method="post">
                        <div class="form-inline">
                            <div class="form-group">
                                <label> title : <input type="text" class="form-control" id="title" name="title" placeholder="store name">
                                </label>
                            </div>
                        </div>
                        <div class="form-inline">
                            <div class="form-group">
                                <label> kind : <input type="text" class="form-control" id="kind" name="kind" placeholder="ex) 일식 전문점">
                                </label>
                            </div>
                        </div>
                        <div class="form-inline">
                            <div class="form-group">
                                <label> address : <input type="text" class="form-control" id="address" name="address" placeholder="address">
                                </label>
                            </div>
                        </div>
                        <div class="form-inline">
                            <div class="form-group">
                                <label> open time : <input type="text" class="form-control" id="opentime" name="opentime" placeholder="opentime">
                                </label>
                            </div>
                        </div>
                        <div class="form-inline">
                            <div class="form-group">
                                <label> close time : <input type="text" class="form-control" id="closetime" name="closetime" placeholder="closetime">
                                </label>
                            </div>
                        </div>
                        <div class="form-inline">
                            <div class="form-group">
                                <label> reservation : 
                                    <div class="radio">
                                        <input type="radio" class="form-control" id="reservation" name="reservation" value="OK"> 가능
                                    </div>
                                    <div class="radio">
                                        <input type="radio" class="form-control" id="reservation" name="reservation" value="NO"> 불가
                                    </div>
                                </label>
                            </div>
                        </div>
                        <div class="form-inline">
                            <div class="form-group">
                                <label> phonenumber : <input type="text" class="form-control" id="phonenumber" name="phonenumber" placeholder="010-xxxx-xxxx">
                                </label>
                            </div>
                        </div>
                        <div class="form-inline">
                            <div class="form-group">
                                <label> images : 
                                </label>
                            </div>
                        </div>
                        <div>
                            <button type="submit" class="form-control btn btn-success">Register</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
</div>
<% include ./footer.ejs %>